{
  "extends": [
    "plugin:vue/recommended"
  ],
  // 2 will report an error, meaning the eslint grunt task will fail.
  // 1 will report a warning, but will not cause the task to fail.
  // All rules that are set to 0 are off.
  "rules": {
    "no-caller": 2,
    "strict": 0,
    "new-cap": 1,
    "quotes": [2, "single", { "allowTemplateLiterals": true }],
    "wrap-iife": [2, "inside"],
    "valid-typeof": 2,
    "valid-jsdoc": [2, { "requireReturn": false }],
    "curly": [0, "multi"],
    "no-console": 1,
    "no-debugger": 2,
    "no-use-before-define": 0,
    "no-new": 0,
    "no-return-assign": 0,
    "no-cond-assign": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "window": false,
    "document": false,
    "module": false,
    "console": false,
    "google": false,
    "require": false,
    "__dirname": false,
    "process": false,
    "exports": false
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  }
}
